  for (int loop = ini; loop >= fim; loop--) {
    digitalWrite(matrizV[col], HIGH);
    digitalWrite(matrizV[col2], HIGH);
    digitalWrite(matrizV[col3], HIGH);
    digitalWrite(matrizH[loop], LOW);

    if (digitalRead(botoes[0]) == HIGH || digitalRead(botoes[1]) == HIGH || digitalRead(botoes[2]) == HIGH || digitalRead(botoes[3]) == HIGH || digitalRead(botoes[4] == HIGH)) {
      contadordclicks++;
    }

    if (loop != ini) {
      digitalWrite(matrizH[loop + 1], HIGH);
    } //desligar o de tras tone 1

    if (loop == 3 || loop == 4) {
      if (digitalRead(botoes[col]) == HIGH && digitalRead(botoes[col2]) == HIGH && digitalRead(botoes[col3]) == HIGH) {
        //valveanticheat = 1;
      } else {
        valveanticheat = 0;
      }
    }


    if (loop == 1) {
      if ((digitalRead(botoes[col]) == HIGH) && (digitalRead(botoes[col2]) == HIGH) && (digitalRead(botoes[col3]) == HIGH && valveanticheat == 0)) {
        //if (verif == 0) {
        chain++;
        scores();
        simounao = 1;
        //}
      }
    }
    if (loop == 0) {
      if (simounao == 0) {
        if ((digitalRead(botoes[col]) == HIGH) && (digitalRead(botoes[col2]) == HIGH) && (digitalRead(botoes[col3]) == HIGH && valveanticheat == 0)) {
          chain++;
          scores();
        } else {
          vida = vida - 1;
          chain = 0;
        }
      }
    }
    escolha4 = Serial.read();
    if (escolha4 == 98) {
      Serial.println("");
      Serial.print("Voce cancelou a musica, voltando para o home...");
      noTone(3);
      delay(1000);
      resetFunc();
    }
    if (loop == 9) {
      simounao = 0;
    }
    vidas();
    int c = temp / (ini - fim + 1);
    delay(c);
  }
  digitalWrite(matrizV[col], LOW);
  digitalWrite(matrizV[col2], LOW);
  digitalWrite(matrizV[col3], LOW);
  digitalWrite(matrizH[0], HIGH);
}

